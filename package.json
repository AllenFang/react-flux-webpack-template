{
  "name": "react-flux-webpack-template",
  "version": "0.1.0",
  "description": "It's a react and flux template with webpack",
  "scripts": {
    "prod": "NODE_ENV=production webpack -p",
    "dev": "webpack-dev-server --devtool eval --progress --hot --colors --content-base ./build",
    "start": "node server/server-dev",
    "test": "jest"
  },
  "author": "AllenFang",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^5.4.2",
    "babel-loader": "^5.0.0",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "css-loader": "^0.12.1",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "jest-cli": "^0.4.5",
    "morgan": "~1.5.1",
    "node-libs-browser": "^0.5.0",
    "node-sass": "^3.1.1",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.2",
    "serve-favicon": "~2.2.0",
    "style-loader": "^0.12.2",
    "webpack": "^1.9.5",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "classnames": "^2.1.1",
    "flux": "^2.0.3",
    "object-assign": "^2.0.0",
    "react": "^0.13.3",
    "whatwg-fetch": "^0.8.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
